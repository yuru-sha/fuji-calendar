{
  "name": "fuji-calendar",
  "version": "0.1.0",
  "description": "ダイヤモンド富士とパール富士のカレンダーアプリケーション",
  "main": "dist/server/index.js",
  "scripts": {
    "dev:server": "nodemon",
    "dev:client": "vite", 
    "dev:worker": "nodemon --exec ts-node src/server/worker.ts",
    "dev": "npm run build:client && npm run dev:server",
    "worker": "node dist/server/worker.js",
    "build:server": "tsc -p tsconfig.server.json",
    "build:client": "vite build",
    "build": "npm run build:server && npm run build:client",
    "start": "node dist/server/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "typecheck": "tsc --noEmit && tsc -p tsconfig.server.json --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "benchmark:cache": "node scripts/cache-benchmark.js",
    "debug:pearl": "node scripts/debug-pearl-fuji.js",
    "test:cache": "node scripts/test-cache-consistency.js",
    "clear:pearl": "node scripts/clear-pearl-cache.js",
    "analyze:performance": "node scripts/performance-analysis.js"
  },
  "keywords": [
    "fuji",
    "calendar",
    "photography",
    "diamond-fuji",
    "pearl-fuji"
  ],
  "author": "yuru-sha",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^6.12.0",
    "@types/pg": "^8.15.4",
    "astronomy-engine": "^2.1.19",
    "bcrypt": "^5.1.1",
    "bullmq": "^5.56.4",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "leaflet": "^1.9.4",
    "pg": "^8.16.3",
    "pino": "^9.7.0",
    "pino-http": "^10.5.0",
    "pino-pretty": "^13.0.0",
    "playwright": "^1.54.1",
    "prisma": "^6.12.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.11",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^13.4.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.16",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/leaflet": "^1.9.8",
    "@types/node": "^20.9.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.1.1",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.2",
    "eslint": "^8.53.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.0.4",
    "nodemon": "^3.0.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.0"
  }
}
